{
  "name": "firebaseauthentication",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "rules": "firebase deploy --only firestore:rules",
    "emulator": "run-script-os",
    "emulator:win32:linux": "cd ./functions&&npm run build&&cd ./..&&firebase emulators:start",
    "emulator:darwin": "ls ./functions&&npm run build&&ls ./..&&firebase emulators:start",
    "test": "run-script-os",
    "test:win32": "SET ISTEST=true&&jest",
    "test:darwin:linux": "export ISTEST=true&&jest"
  },
  "dependencies": {
    "@babel/core": "^7.12.17",
    "@types/jest": "^26.0.20",
    "axios": "^0.21.1",
    "babel-jest": "^26.6.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "firebase": "^8.2.7",
    "firebase-admin": "^9.5.0",
    "firebase-functions": "^3.13.2",
    "jest": "^26.6.3",
    "jest-enzyme": "^7.1.2",
    "next": "10.0.7",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "run-script-os": "^1.1.5"
  },
  "devDependencies": {
    "@types/node": "^14.14.28",
    "@types/react": "^17.0.2",
    "typescript": "^4.1.5"
  }
